{
  "env": {
    "browser": true
  },
  "extends": "eslint:recommended",
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "globals": {
    "module": true
  },
  "rules": {
    "no-unused-vars": "off",
    "no-undef": "off",
    "no-debugger": "off"
  }
}
